{
    "extends": "google",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
    },
    "rules": {
        "no-invalid-this": "off",
        "indent": ["error", 4],
        "space-before-function-paren": "off",
        "operator-linebreak": ["error", "before"],
        "require-jsdoc": "off",
        "max-len": ["error", {"code": 100}]
    }
}
